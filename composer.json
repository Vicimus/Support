{
  "name": "vicimus/support",
  "type": "library",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "vicimus/standard": "~3",
    "phpunit/phpunit": "~6",
    "laravel/lumen": "~5",
    "illuminate/database": "~5",
    "illuminate/http": "~5",
    "illuminate/validation": "~5",
    "illuminate/auth": "~5",
    "guzzlehttp/guzzle": "~6"
  },
  "suggest": {
    "laravel/lumen": "Required to use LumenRouter",
    "illuminate/database": "Required to use Vicimus Model",
    "illuminate/http": "Required for use with many classes",
    "illuminate/validation": "Required for ImmutableObject and others",
    "illuminate/auth": "Required for Authentication service",
    "guzzlehttp/guzzle": "Required for API Service class"
  },
  "autoload": {
    "psr-4": {
      "Vicimus\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packages.vicimus.com"
    }
  ],
  "config": {
    "sort-packages": true
  }
}
