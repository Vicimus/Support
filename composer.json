{
    "name": "vicimus/support",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "illuminate/database": "~5",
        "illuminate/http": "~5",
        "illuminate/validation": "~5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3",
        "vicimus/standard": "~2",
        "phpunit/phpunit":"~6"
    },
    "autoload": {
        "psr-4": {
            "Vicimus\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@bitbucket.org:vicimus/ymlcollection.git"
        }
    ]
}
