{
    "name": "vicimus/support",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "guzzle/guzzle": "^3.9",
        "symfony/yaml": "^3.1",
        "vicimus/ymlcollection": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2",
        "vicimus/standard": "~1",
        "phpunit/phpunit":"~5"
    },
    "autoload": {
        "psr-4": {
            "Vicimus\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@bitbucket.org:vicimus/ymlcollection.git"
        }
    ]
}
