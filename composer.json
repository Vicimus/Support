{
  "name": "vicimus/support",
  "type": "library",
  "require": {
      "php": ">=7.1",
      "ext-json": "*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "~6",
    "illuminate/auth": "~5",
    "illuminate/database": "~5",
    "illuminate/http": "~5",
    "illuminate/validation": "~5",
    "laravel/lumen": "~5",
    "mikey179/vfsStream": "^1.6",
    "phpunit/phpunit": "~7",
    "vicimus/standard": "~4"
  },
  "suggest": {
      "laravel/lumen": "Required to use LumenRouter",
      "illuminate/database": "Required to use Vicimus Model",
      "illuminate/http": "Required for use with many classes",
      "illuminate/validation": "Required for ImmutableObject and others",
      "illuminate/auth": "Required for Authentication service",
      "guzzlehttp/guzzle": "Required for API Service class",
      "symfony/dom-crawler": "Required for using the locale sniffer"
  },
  "autoload": {
    "files": ["src/helpers.php"],
    "psr-4": {
      "Vicimus\\Support\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packages.vicimus.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
    "description": "Many support classes and interfaces"
}
