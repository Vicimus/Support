{
    "name": "vicimus/support",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6",
        "illuminate/database": "~5",
        "illuminate/http": "~5",
        "illuminate/validation": "~5",
        "illuminate/auth": "~5"
    },
    "require-dev": {
        "vicimus/standard": "~3",
        "phpunit/phpunit":"~6"
    },
    "autoload": {
        "psr-4": {
            "Vicimus\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "repositories": [
      { "type": "composer", "url": "https://repo.packages.vicimus.com" }
    ]
}
